module core;

public abstract type Stack[+T] extends Sequence[T] {
  public T getBack();
}

public abstract type MutableStack[T] extends Stack[T] {
  public Unit pushBack(T elem);
  public T popBack();
}

public abstract type ImmutableStack[T] extends Stack[T] {
  public Stack[T] pushBack(T elem);
  public Stack[T] popBack(T elem);
}
