module core;

type Tuple2[+A, +B] {
  A a;
  B b;

  Void init(A a, B b) {
    this.a = a;
    this.b = b;
  }

  A first() {
    return this.a;
  }

  B second() {
    return this.b;
  }
}

type Tuple3[+A, +B, +C] {
  A a;
  B b;
  C c;

  Void init(A a, B b, C c) {
    this.a = a;
    this.b = b;
    this.c = c;
  }

  A first() {
    return this.a;
  }

  B second() {
    return this.b;
  }

  C third() {
    return this.c;
  }
}
