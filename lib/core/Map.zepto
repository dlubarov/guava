module core;

abstract type Map[-K, +V] extends Collection[Tuple2[K, V]] {
  Maybe[V] get(K key);
}

abstract type MutableMap[K, V] extends Map[K, V] {
  # Returns the given val, NOT the old val (as Java's Map would)
  V set(K key, V val);
}

abstract type ImmutableMap[-K, +V] extends Map[K, V] {
}
