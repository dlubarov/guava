module core;

public abstract type Queue[+T] extends Sequence[T] {
}

public abstract type MutableQueue[T] extends Queue[T], MutableSequence[T] {
  public Unit pushBack(T value);
  public T popFront();
}

public abstract type ImmutableQueue[T] extends Queue[T], ImmutableSequence[T] {
  public ImmutableQueue[T] pushBack(T value);
  public ImmutableQueue[T] popFront();
}
