module core;

public abstract type Queue[+T] extends Sequence[T] {
}

public abstract type MutableQueue[T] extends Queue[T] {
  public Unit pushBack(T elem);
  public T popFront();
}

public abstract type ImmutableQueue[T] extends Queue[T] {
  public ImmutableQueue[T] pushBack(T elem);
  public ImmutableQueue[T] popFront();
}
