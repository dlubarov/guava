module core;

# A Set is a collection which contains no duplicate elements.
abstract type Set[+T] extends Collection[T] {
  Set[T] &(Set[T] that);

  Set[T] |(Set[T] that);

  Set[T] ^(Set[T] that);

  Int count(T elem) {
    if (this.contains(elem))
      return 1;
    else
      return 0;
  }

  Set[T] uniqueElems() {
    return this;
  }
}

abstract type MutableSet[T] extends Set[T] {
  Void +=(T elem);
}

abstract type ImmutableSet[+T] extends Set[T] {
}
