module test;

type ProgressBar {
  static Unit main(Sequence[String] args) {
    Int w = 70;
    for (Int i = 0; i <= w; i = i + 1) {
      Console.out("\r");
      Console.out("|" * i);
      Console.out("-" * (w - i));
      Console.out(" (" + i + "/" + w + ")");
    }
    Console.out("\n");
  }
}
